colorscheme darkblue
syntax on
set sw=2
set expandtab
set softtabstop=2
set smarttab
set autoindent
set smartindent
set tabstop=2
set smartindent
set noswapfile
set ruler
set nowrap
set nocompatible
set nobackup
set nowritebackup
set whichwrap+=<,>,[,]
set scrolloff=6
set list
set listchars=eol:$,tab:>.
